/// <reference types="cypress" />

describe('', () => {
  before('', () => {
    cy.visit('http://react-redux.realworld.io/#/?_k=e6iq14');
  });

  it('', () => {
    cy.get(':nth-child(2) > .nav-link').click();
    cy.get(':nth-child(1) > .form-control').type("femozi@mailinator.com")
    cy.get(':nth-child(2) > .form-control').type("TGFRbvh456")
    cy.get('.btn').click();
    cy.get('.navbar').contains('Consequatur esse do').should('exist');
  })
});
